   "source": [
    "def fact(n):\n",
    "    f=1\n",
    "    for i in range(1,n+1):\n",
    "        f=f*i\n",
    "    return f\n",
    "\n",
    "n = int(input('Enter the Number: '))\n",
    "\n",
    "res=fact(n)\n",
    "print(res)"
   ]
