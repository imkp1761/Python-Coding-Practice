    "def fact(n):\n",
    "    \n",
    "    if n==0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n*fact(n-1)\n",
    "    \n",
    "n=int(input('Enter the number: '))\n",
    "\n",
    "res=fact(n)\n",
    "print(res)"
   ]
